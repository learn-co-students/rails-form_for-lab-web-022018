<h1> Student Form: </h1>
<!-- <form action="<%= school_classes_path %>", method="POST"> -->
<%= form_tag students_path do %>
  <label>Student First Name:</label><br>
  <!-- <input type="text", id="school_class_title", name="school_class[title]"><br> -->
  <%= text_field_tag :'student[first_name]' %><br>
  <label>Student Last Name:</label><br>
  <!-- <input type="text", id="school_class_room_number", name="school_class[room_number]"><br> -->
  <%= text_field_tag :'student[last_name]' %><br>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <!-- <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>"> -->
  <%= submit_tag "Create Student" %>
  <!-- <input type="submit" value="Create School class"> -->
<% end %>
<%= params.inspect %>
