<h1> School Class Form: </h1>
<!-- <form action="<%= school_classes_path %>", method="POST"> -->
<%= form_tag school_classes_path do %>
  <label>School Class Title:</label><br>
  <!-- <input type="text", id="school_class_title", name="school_class[title]"><br> -->
  <%= text_field_tag :'school_class[title]' %><br>
  <label>School Class Room Number:</label><br>
  <!-- <input type="text", id="school_class_room_number", name="school_class[room_number]"><br> -->
  <%= text_field_tag :'school_class[room_number]' %><br>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <!-- <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>"> -->
  <%= submit_tag "Create School class" %>
  <!-- <input type="submit" value="Create School class"> -->
<% end %>
<%= params.inspect %>
