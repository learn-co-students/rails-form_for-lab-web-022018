<h1> Edit Class Form: </h1>
<%= form_for(@school_class) do |f| %>
  <label>School Class Title:</label><br>
  <%= f.text_field :title %><br>
  <label>School Class Room Number:</label><br>
  <%= f.text_field :room_number %><br>
  <%= f.submit %>
<% end %>
